<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/result_yellow_color"/>
            <corners android:radius="@dimen/round_btn_corner_radius"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:color="@color/result_yellow_color" android:width="1dip"/>
            <solid android:color="@color/transparent"/>
            <corners android:radius="@dimen/round_btn_corner_radius"/>
        </shape>
    </item>
</selector>